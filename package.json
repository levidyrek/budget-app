{
  "name": "budgetapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/codemod": "^1.1.0",
    "@material-ui/core": "^3.2.0",
    "ajv": "^6.5.4",
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.20.2",
    "react": "^16.5.2",
    "react-data-grid": "^3.0.11",
    "react-dom": "^16.5.2",
    "react-loading": "^1.0.5",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.3.1",
    "react-signup-login-component": "^1.2.0",
    "react-table": "^6.8.6",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
