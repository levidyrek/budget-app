  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@material-ui/codemod": "^1.1.0",
    "@material-ui/core": "^3.7.0",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "ajv": "^6.6.1",
    "detect-browser": "^4.2.0",
    "email-validator": "^2.0.4",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.20.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-data-grid": "^3.0.11",
    "react-dom": "^16.6.3",
    "react-loading": "^1.0.5",
    "react-number-format": "^3.6.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.2",
    "react-signup-login-component": "^1.2.0",
    "react-table": "^6.8.6",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/eslint/bin/eslint.js src/**/*.js"
  }
}
