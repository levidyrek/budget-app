{
  "name": "budgetapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "es6-promise": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-data-grid": "^3.0.11",
    "react-dom": "^16.2.0",
    "react-loading": "^1.0.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-signup-login-component": "^1.0.3",
    "react-table": "^6.8.0",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.2.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
